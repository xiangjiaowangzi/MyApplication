<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.activitydemo">

    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name" android:supportsRtl="true" android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.TestLaunchActivity"
            android:label="TestLaunchActivity"
            android:launchMode="standard"
            />
        <activity android:name=".activity.StandardActivity"
            android:label="StandardActivity"
            android:launchMode="standard"
            />
        <activity android:name=".activity.SinggleTopActivity"
            android:label="SinggleTopActivity"
            android:launchMode="singleTop"
            />
        <activity android:name=".activity.SingleTaskActivity"
            android:label="SingleTaskActivity"
            android:launchMode="singleTask"
            />
        <activity android:name=".activity.singleInstanceActivity"
            android:label="singleInstanceActivity"
            android:launchMode="singleInstance"
            />
    </application>

</manifest>