        </activity>

        <activity android:name=".TestMesureViewAct"/>
        <activity android:name=".TestTextAct"/>
        <activity android:name=".TestSwipeAct"/>
        <activity android:name=".TestTouchAct"/>
        <activity android:name=".TestNestedScrollAct"/>
        <activity android:name=".TestScrollViewAct"/>
    </application>

</manifest>